<div data-role="page" data-add-back-btn="true">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1><%= @neighborhood.id %></h1>
    <%= link_to "Edit", :action => :edit, :id => @neighborhood.object %>
  </div>

  <div data-role="content">
    <table>
      
      <tr>
    	<td class="itemLabel">Id</td>
        <td class="itemValue"><%= @neighborhood.id%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Url</td>
        <td class="itemValue"><%= @neighborhood.url%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Lat</td>
        <td class="itemValue"><%= @neighborhood.lat%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Long</td>
        <td class="itemValue"><%= @neighborhood.long%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Zip</td>
        <td class="itemValue"><%= @neighborhood.zip%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Name</td>
        <td class="itemValue"><%= @neighborhood.name%></td>
      </tr>
      
    </table>
  </div>

</div>
